*&---------------------------------------------------------------------*
*& text
*&---------------------------------------------------------------------*
*& -->  p1        text
*& <--  p2        text
*&---------------------------------------------------------------------*
FORM f_exibe_param .

  CASE abap_true.
    WHEN p_cfop_e.
      PERFORM f_busca_cfop_e.
    WHEN p_cfop_f.
      PERFORM f_busca_cfop_f.
    WHEN p_cfop_t. "CFOP Entrada por Transferência
      PERFORM f_busca_cfop_t.
    WHEN p_cfop_l.
      PERFORM f_busca_cfop_l.
    WHEN p_cfop_r.
      PERFORM f_busca_cfop_r.
    WHEN p_matkl.
      PERFORM f_busca_matkl.
    WHEN p_fkart.
      PERFORM f_busca_fkart.
    WHEN p_fkarti.
      PERFORM f_busca_fkart_ind.
    WHEN p_final.
      PERFORM f_busca_finalidade.
    WHEN p_fdev_e. "Finalid RFL-Dev NF Ent p/ Fila
      PERFORM f_busca_fdev_e.
    WHEN p_user_f.
      PERFORM f_busca_user_form_lote.
    WHEN p_ativa.
      PERFORM f_ativa_regra_form_lote.
    WHEN p_ufxml. "UF Vínculo NF Ent. X Saída XML
      PERFORM f_busca_ufxml.
*   US #180203 - MMSILVA - 26.05.2025 - Inicio
    WHEN p_aprov.
      PERFORM f_ativa_desativa_aprov.
*   US #180203 - MMSILVA - 26.05.2025 - Fim
    WHEN OTHERS.
  ENDCASE.
ENDFORM.
