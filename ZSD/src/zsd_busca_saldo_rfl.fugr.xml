       <TYP>ZSDT_RFL_SALDO_FISICO</TYP>
      </RSEXP>
     </EXPORT>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>I_DOCNUM_PROD</PARAMETER>
       <KIND>P</KIND>
