sap.ui.define([                                                                                                                                                                                                                                                
    "sap/m/MessageToast"                                                                                                                                                                                                                                       
], function(MessageToast) {                                                                                                                                                                                                                                    
    'use strict';                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
    return {                                                                                                                                                                                                                                                   
       onAfterRendering: function () {                                                                                                                                                                                                                         
            this.getView().byId("listReportFilter").addEventDelegate({                                                                                                                                                                                         
                onAfterRendering: function (oEvent) {                                                                                                                                                                                                          
                    for (let oObject of oEvent.srcControl.getContent()[0].getContent()) {                                                                                                                                                                      
                        for (let oChild of oObject.getContent()) {                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                            if (oChild.getId().includes('BASIC'))                                                                                                                                                                                              
                                switch (oChild.getId().split('-').pop()) {                                                                                                                                                                                     
                                    case 'distrib_status':                                                                                                                                                                                                     
                                        debugger;                                                                                                                                                                                                              
                                        oChild.setTokens([                                                                                                                                                                                                     
                                            new sap.m.Token({ text: "2", key: "2" }),                                                                                                                                                                          
                                            new sap.m.Token({ text: "5", key: "5" })                                                                                                                                                                           
                                        ]);                                                                                                                                                                                                                    
                                        break;                                                                                                                                                                                                                 
                                    default:                                                                                                                                                                                                                   
                                        break;                                                                                                                                                                                                                 
                                }                                                                                                                                                                                                                              
                        }                                                                                                                                                                                                                                      
                    }                                                                                                                                                                                                                                          
                }.bind(this)                                                                                                                                                                                                                                   
            })                                                                                                                                                                                                                                                 
        },                                                                                                                                                                                                                                                     
    };                                                                                                                                                                                                                                                         
});                                                                                                                                                                                                                                                            