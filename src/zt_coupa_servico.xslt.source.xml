<?sap.transform simple?>
<tt:transform xmlns:tt="http://www.sap.com/transformation-templates" xmlns:ddic="http://www.sap.com/abapxml/types/dictionary" xmlns:def="http://www.sap.com/abapxml/types/defined">
  <tt:root name="ROOT" type="?"/>
  <tt:root name="INVENTORY-TRANSACTION" type="ddic:ZCOUPA_TT_SERVICOS"/>
  <tt:template>
    <inventory-transactions>
      <tt:loop ref="INVENTORY-TRANSACTION">
        <inventory-transaction>
          <id tt:value-ref="ID"/>
          <price tt:value-ref="PRICE"/>
          <quantity tt:value-ref="QUANTITY"/>
          <total tt:value-ref="TOTAL"/>
          <transaction-date tt:value-ref="TRANSACTION_DATE"/>
          <type tt:value-ref="TYPE"/>
          <status tt:value-ref="STATUS"/>
          <match-reference tt:value-ref="MATCH_REFERENCE"/>
          <original-transaction-id tt:value-ref="ORIGINAL_TRANSACTION_ID"/>
          <voided-value tt:value-ref="VOIDED_VALUE"/>
          <exported tt:value-ref="EXPORTED"/>

<!--          <account>
            <id tt:value-ref="ACCOUNT.ID"/>
            <name tt:value-ref="ACCOUNT.NAME"/>
            <code tt:value-ref="ACCOUNT.CODE"/>
            <active tt:value-ref="ACCOUNT.ACTIVE"/>
            <account-type-id tt:value-ref="ACCOUNT.ACCOUNT_TYPE_ID"/>
            <segment-1 tt:value-ref="ACCOUNT.SEGMENT_1"/>
            <segment-2 tt:value-ref="ACCOUNT.SEGMENT_2"/>
            <segment-3 tt:value-ref="ACCOUNT.SEGMENT_3"/>
            <segment-4 tt:value-ref="ACCOUNT.SEGMENT_4"/>
          </account>-->

<!--          <account-allocations>
            <tt:loop ref="ACCOUNT_ALLOCATIONS">
              </tt:loop>
          </account-allocations>-->

          <order-line>
            <id tt:value-ref="ORDER_LINE.ID"/>
            <line-num tt:value-ref="ORDER_LINE.LINE_NUM"/>
            <order-header-id tt:value-ref="ORDER_LINE.ORDER_HEADER_ID"/>
            <order-header-number tt:value-ref="ORDER_LINE.ORDER_HEADER_NUMBER"/>
          </order-line>

          <item>
            <id tt:value-ref="ITEM.ID"/>
            <item-number tt:value-ref="ITEM.ITEM_NUMBER"/>
            <name tt:value-ref="ITEM.NAME"/>
          </item>

          <uom>
            <code tt:value-ref="UOM.CODE"/>
            <name tt:value-ref="UOM.NAME"/>
          </uom>

          <created-by>
            <id tt:value-ref="CREATED_BY.ID"/>
            <login tt:value-ref="CREATED_BY.LOGIN"/>
            <email tt:value-ref="CREATED_BY.EMAIL"/>
          </created-by>

          <updated-by>
            <id tt:value-ref="UPDATE_BY.ID"/>
            <login tt:value-ref="UPDATE_BY.LOGIN"/>
            <email tt:value-ref="UPDATE_BY.EMAIL"/>
          </updated-by>

        </inventory-transaction>
      </tt:loop>
    </inventory-transactions>
  </tt:template>
</tt:transform>
