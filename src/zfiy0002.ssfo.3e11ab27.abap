cLEAR: V_IMP     ,
V_ACUM    ,
V_ALICUOTA,
V_RETN    ,
V_AFIP    ,
V_FECHA   ,
V_MNI     ,
V_HWSTE_OUT,
V_HWSTE_IVA,
V_DMBTR.

v_ret = GS_RET_POS-RETENCION  + v_ret.

WRITE:
GS_RET_POS-IMP_BASE  TO V_IMP     ,
GS_RET_POS-ALICUOTA  TO V_ALICUOTA DECIMALS 2,
GS_RET_POS-RETENCION TO V_RETN    ,
GS_RET_POS-BASE_ACUM TO V_ACUM    ,
GS_RET_POS-FECHA     TO V_FECHA   ,
GS_RET_POS-COD_FIP   TO V_AFIP    ,
GS_RET_POS-MNI       TO V_MNI     ,
GS_RET_POS-HWSTE_OUT TO V_HWSTE_OUT,
GS_RET_POS-HWSTE_IVA TO V_HWSTE_IVA,
GS_RET_POS-DMBTR     to V_DMBTR.

CONDENSE V_IMP      NO-GAPS.
CONDENSE V_ACUM     NO-GAPS.
CONDENSE V_RETN     NO-GAPS.
CONDENSE V_AFIP     NO-GAPS.
CONDENSE V_MNI      NO-GAPS.
CONDENSE V_HWSTE_OUT NO-GAPS.
CONDENSE V_HWSTE_IVA NO-GAPS.
CONDENSE V_ALICUOTA NO-GAPS.
CONDENSE V_DMBTR NO-GAPS.














