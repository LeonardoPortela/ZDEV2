      READ TABLE T_MARA_CT INTO WA_MARA_CT WITH KEY  MATNR = WA_J_1BNFLIN_CT-MATNR.
      IF SY-SUBRC = 0.
        WA_SAIDA-GRUPO_MER = WA_MARA_CT-MATKL.
