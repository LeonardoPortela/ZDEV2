"Name: \FU:J_1B_BRANCH_READ\SE:END\EI
ENHANCEMENT 0 Z_FUSO_HORARIO_READ_BRANCH.

  DATA: V_TIME_ZONE TYPE TZNZONE.

  CALL FUNCTION 'Z_FUSO_HORARIO_FILIAL'
    EXPORTING
      I_BUKRS    = J_1BBRANCH-BUKRS
      I_BRANCH   = J_1BBRANCH-BRANCH
    IMPORTING
      E_TZNZONE  = V_TIME_ZONE.

  IF V_TIME_ZONE IS NOT INITIAL.
    ADDRESS-TZONE           = V_TIME_ZONE.
    ADDRESS_VALUE-TIME_ZONE = V_TIME_ZONE.
  ENDIF.

ENDENHANCEMENT.
