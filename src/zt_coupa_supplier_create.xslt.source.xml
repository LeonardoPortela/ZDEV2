<?sap.transform simple?>
<tt:transform xmlns:tt="http://www.sap.com/transformation-templates" xmlns:ddic="http://www.sap.com/abapxml/types/dictionary" xmlns:def="http://www.sap.com/abapxml/types/defined">
  <tt:root name="ROOT" type="?"/>
  <tt:root name="SUPPLIER" type="ddic:ZCOUPA_SUPPLIER_CREATE"/>
  <tt:template>
    <supplier>
      <name tt:value-ref=".SUPPLIER.NAME"/>
      <display-name tt:value-ref=".SUPPLIER.DISPLAY_NAME"/>
      <number tt:value-ref=".SUPPLIER.NUMBER"/>
      <status tt:value-ref=".SUPPLIER.STATUS"/>
      <po-method tt:value-ref=".SUPPLIER.PO_METHOD"/>
      <account-number tt:value-ref=".SUPPLIER.ACCOUNT_NUMBER"/>
      <po-email tt:value-ref=".SUPPLIER.PO_EMAIL"/>
      <tax-id tt:value-ref=".SUPPLIER.TAX_ID"/>
      <buyer-hold tt:value-ref=".SUPPLIER.BUYER_HOLD"/>
      <on-hold tt:value-ref=".SUPPLIER.ON_HOLD"/>
      <invoice-matching-level tt:value-ref=".SUPPLIER.INVOICE_MATCHING_LEVEL"/>
      <custom-fields>
        <razo-social tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.RAZO_SOCIAL"/>
        <tipo-de-fornecedor>
          <external-ref-code tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.TIPO_DE_FORNECEDOR.EXTERNAL_REF_CODE"/>
        </tipo-de-fornecedor>
        <status-homologacao>
          <external-ref-code tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.STATUS_HOMOLOGACAO.EXTERNAL_REF_CODE"/>
        </status-homologacao>
        <empresas tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.EMPRESAS"/>
        <inscricao-estadual tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.INSCRICAO_ESTADUAL"/>
        <email-nota-fiscal tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.EMAIL_NOTA_FISCAL"/>
        <rg tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.RG"/>
        <orgao-expedidor tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.ORGAO_EXPEDIDOR"/>
        <pis-nit tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.PIS_NIT"/>
        <matrcula-funcionrio tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.MATRCULA_FUNCIONRIO"/>
        <no-possui-ie-ou-isento tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.NO_POSSUI_IE_OU_ISENTO"/>
        <forma-de-tratamento tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.FORMA_DE_TRATAMENTO"/>
        <organizao-de-compras tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.ORGANIZAO_DE_COMPRAS"/>
<!--        <cpf tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.CPF"/>-->
        <cnpjcpfvat tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.CNPJCPFVAT"/>
<!--        <cnpj tt:value-ref=".SUPPLIER.CUSTOM_FIELDS.CNPJ"/>-->
      </custom-fields>
      <primary-contact>
        <email tt:value-ref=".SUPPLIER.PRIMARY_CONTACT.EMAIL"/>
        <name-give tt:value-ref=".SUPPLIER.PRIMARY_CONTACT.NAME_GIVEN"/>
        <name-family tt:value-ref=".SUPPLIER.PRIMARY_CONTACT.NAME_FAMILY"/>
      </primary-contact>
      <supplier-addresses>
        <tt:loop ref=".SUPPLIER.SUPPLIER_ADDRESSES">
          <supplier-address>
            <street1 tt:value-ref="STREET1"/>
            <street2 tt:value-ref="STREET2"/>
            <city tt:value-ref="CITY"/>
            <state tt:value-ref="STATE"/>
            <postal-code tt:value-ref="POSTAL_CODE"/>
          </supplier-address>
        </tt:loop>
      </supplier-addresses>
    </supplier>
  </tt:template>
</tt:transform>
