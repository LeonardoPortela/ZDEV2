<xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
  <xsl:output encoding="UTF-8" indent="yes" method="xml" omit-xml-declaration="no" version="1.0"/>
<!--  <xsl:strip-space elements="*"/>-->
  <xsl:template match="/">
    <xsl:apply-templates select="//ISWIFT/item"/>
  </xsl:template>
  <xsl:template match="ISWIFT/item">
    <DataPDU xmlns="urn:swift:saa:xsd:saa.2.0">
      <Revision>
        <xsl:value-of select="REVISION"/>
      </Revision>
      <Header>
        <Message>
          <SenderReference>
            <xsl:value-of select="SENDER_REF"/>
          </SenderReference>
          <MessageIdentifier>
            <xsl:value-of select="MESSAGEIDENTIFIER"/>
          </MessageIdentifier>
          <Format>
            <xsl:value-of select="FORMAT"/>
          </Format>
          <Sender>
            <DN>
              <xsl:value-of select="DN_EMPRESA"/>
            </DN>
            <FullName>
              <X1>
                <xsl:value-of select="BC_EMPRESA"/>
              </X1>
            </FullName>
          </Sender>
          <Receiver>
            <DN>
              <xsl:value-of select="DN_BANCO"/>
            </DN>
            <FullName>
              <X1>
                <xsl:value-of select="BC_BANCO"/>
              </X1>
            </FullName>
          </Receiver>
          <NetworkInfo>
            <IsNotificationRequested>
              <xsl:value-of select="NOTREQUE"/>
            </IsNotificationRequested>
            <Service>
              <xsl:value-of select="SERVICE"/>
            </Service>
            <SWIFTNetNetworkInfo>
              <NotificationResponderDN>
                <xsl:value-of select="NTRESPONDER"/>
              </NotificationResponderDN>
              <FileDescription>
                <xsl:value-of select="FILEDESCRIPTION"/>
              </FileDescription>
              <FileInfo>
                <xsl:value-of select="FILEINFO"/>
              </FileInfo>
            </SWIFTNetNetworkInfo>
          </NetworkInfo>
          <SecurityInfo>
            <SWIFTNetSecurityInfo>
              <FileDigestAlgorithm>
                <xsl:value-of select="FILEDIGEST"/>
              </FileDigestAlgorithm>
            </SWIFTNetSecurityInfo>
          </SecurityInfo>
          <FileLogicalName>
            <xsl:value-of select="FILELGNAME"/>
          </FileLogicalName>
        </Message>
      </Header>
      <Body>
        <xsl:value-of select="BODY"/>
      </Body>
    </DataPDU>
  </xsl:template>
</xsl:transform>
