
MODULE USER_COMMAND_0100 INPUT.

  CASE SY-UCOMM.
    WHEN 'BACK'.
      LEAVE TO SCREEN 0.
    WHEN 'EXIT'.
      LEAVE PROGRAM.
    WHEN 'PROC_ENT'.
      PERFORM F_PROCESSAR_ENTRADAS.
    WHEN 'REFRESH'.
      PERFORM F_SELECIONAR_DADOS.
      PERFORM F_PROCESSAR_DADOS.
      CALL METHOD OBJ_ALV_0100->REFRESH_TABLE_DISPLAY
         EXPORTING
           IS_STABLE = WA_STABLE.

  ENDCASE.

ENDMODULE.

MODULE USER_COMMAND_0101 INPUT.
  CASE SY-UCOMM.
    WHEN 'CONFIRM' OR 'CANCEL'.

      IF OBJ_ALV_0101 IS NOT INITIAL.
        CALL METHOD OBJ_ALV_0101->FREE.
        CALL METHOD CL_GUI_CFW=>FLUSH.
        FREE: OBJ_ALV_0101.
      ENDIF.

      LEAVE TO SCREEN 0.

  ENDCASE.

ENDMODULE.
