    <item>
     <DOKIL>
      <ID>FU</ID>
      <OBJECT>ZRFFOM100_INTERBANKING_ARG_2</OBJECT>
      <LANGU>D</LANGU>
      <TYP>T</TYP>
      <VERSION>0011</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00032</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>ZRFFOM100_INTERBANKING_ARG_2</TDNAME>
      <TDID>FU</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00011</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;TEXT&amp;  = &apos;DTA, Zahlungsauftrag&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;EVENT&amp; = &apos;00002020&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;TYPE&amp;  = &apos;PROCESS&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE FI_OPEN_FI_DOCUMENTATION_1 OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>PROCESS</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Dieser Prozeß ist Bestandteil des Reports RFFOM100 zur Erzeugung von</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Zahlungsträgern in einem speziellen Format, das von dem Standardformat</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>MT100 abweicht. Er wird aufgerufen zum Zeitpunkt WRITE_DATA und dient</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>daher zum Erzeugen des Transaction Records. Der Aufruf erfolgt nur,</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>wenn auf dem Selektionsbild des Reports RFFOM100 ein abweichendes</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Format angegeben wurde und in der Sicht  &quot;Zeitpunkte für MT100- und</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>andere DTA-Formate&quot; (View V_T042OFI) für dieses Format das Ankreuzfeld</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&quot;Baustein für Record erforderlich&quot; gesetzt ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>SCHNITTSTELLE</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>I_FORMAT</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Bezeichnung des abweichenden Formats;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>I_REGUH</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Struktur mit Regulierungsdaten aus dem Zahlprogramm auf Ebene des</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Zahlungsbeleges;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>I_DTAM100</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Diese Struktur enthält ausgewählte Felder des Formats MT100 mit den von</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>SAP vorgesehenen Werten.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>E_DTAM100</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>Exportstruktur für den MT100-Datenrecord; In dieser Struktur werden die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>vom Prozeßbaustein veränderten Felder für das Format MT100</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zurückgeliefert.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>T_REGUP</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Die Tabelle T_REGUP enthält die zu dem Eintrag in der Struktur I_REGUH</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>gehörigen bearbeiteten Positionen aus dem Zahlprogramm.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>T_DTAM100V</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>Die Tabelle T_DTAM100V dient zum Export des im Prozeßbaustein erzeugten</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&apos;Transaction Records&apos;.</TDLINE>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>FU</ID>
      <OBJECT>ZRFFOM100_INTERBANKING_ARG_2</OBJECT>
      <LANGU>E</LANGU>
      <TYP>T</TYP>
      <VERSION>0011</VERSION>
      <TXTLINES>00033</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>ZRFFOM100_INTERBANKING_ARG_2</TDNAME>
      <TDID>FU</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00011</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;TEXT&amp;  = &apos;DME, payment order&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;EVENT&amp; = &apos;00002020&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;TYPE&amp;  = &apos;PROCESS&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE FI_OPEN_FI_DOCUMENTATION_1 OBJECT DOKU ID TX LANGUAGE EN</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>PROCESS</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>This process is part of report program RFFOM100, used for creating</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>payment media in a special format that differs from the standard format</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>MT100. It is called up at the Event WRITE_DATA and is used for creating</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>the transaction record. It is only called up if an alternative format</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>has been specified on the selection screen of report program RFFOM100</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>and if the &quot;Function module required for record&quot; checkbox has been</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>selected for that format in the view &quot;Events for MT100 and other DME</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>formats&quot; (View V_T042OFI).</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>INTERFACE</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>I_FORMAT</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Description of alternative format.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>I_REGUH</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Structure with payment data from the payment program at payment</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>document level.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>I_DTAM100</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>This structure contains selected fields from format MT100 with the</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>values as set by SAP.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>E_DTAM100</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Export struture for the MT100 data record; the fields changed by the</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>maximum process for format MT100 are transferred back in this</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>structure.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>T_REGUP</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Table T_REGUP contains those line items from the payment program that</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>are assigned to the entry in the structure I_REGUH and have already</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>been processed.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>EL</TDFORMAT>
       <TDLINE>T_DTAM100V</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>ER</TDFORMAT>
       <TDLINE>Table T_DTAM100V is used to export the transaction record created in</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>the maximum process.</TDLINE>
      </TLINE>
     </LINES>
    </item>
   </LONGTEXTS_ZRFFOM100_INTERBANKING_ARG_2>
