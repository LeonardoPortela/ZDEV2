
CLEAR: GV_TRANSP_MODE.

CASE I_IDE_DACTE-MODAL.
  WHEN '01'.
    GV_TRANSP_MODE = 'RODOVIÁRIO'.
  WHEN '02'.
    GV_TRANSP_MODE = 'AÉREO'.
  WHEN '03'.
    GV_TRANSP_MODE = 'AQUAVIÁRIO'.
  WHEN '04'.
    GV_TRANSP_MODE = 'FERROVIÁRIO'.
  WHEN '05'.
    GV_TRANSP_MODE = 'DUTOVIÁRIO'.
  WHEN '06'.
    GV_TRANSP_MODE = 'MULTIMODAL'.
ENDCASE.
























