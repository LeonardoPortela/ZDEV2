FUNCTION ZLES_CADASTRO_AVSEG_TOKEN.
*"----------------------------------------------------------------------
*"*"Interface local:
*"  IMPORTING
*"     REFERENCE(I_FILTRO) TYPE  ZDE_ZLEST0145_FILTRO
*"----------------------------------------------------------------------

  DATA: OBJETO         TYPE REF TO ZCL_AVERBACAO_SEGURO_TOKEN,
        IT_ZLEST0145_R TYPE ZDE_ZLEST0145_T.

  CREATE OBJECT OBJETO.

  OBJETO->ZIF_PESQUISA~PESQUISAR( EXPORTING I_FILTROS = I_FILTRO IMPORTING E_REGISTROS = IT_ZLEST0145_R ).
  LOOP AT IT_ZLEST0145_R INTO DATA(WA_ZLEST0145_R).
    APPEND WA_ZLEST0145_R TO IT_ZLEST0145.
  ENDLOOP.
  LC_FILTRO_45 = I_FILTRO.
  CALL SCREEN 0002.

  CLEAR: OBJETO.

ENDFUNCTION.
