FUNCTION Z_SD_INBOUND_FRETE_FOB_MESA.
*"----------------------------------------------------------------------
*"*"Interface local:
*"  TABLES
*"      T_ZIB_CONT_FRETE STRUCTURE  ZIB_CONT_FRETE
*"      T_ZIB_CONT_FRETE_PARC STRUCTURE  ZIB_CONT_FRETE_PARC
*"----------------------------------------------------------------------


  DATA : WA_ZIB_CONT_FRETE      TYPE ZIB_CONT_FRETE     ,
         WA_ZIB_CONT_FRETE_PARC TYPE ZIB_CONT_FRETE_PARC,
         WA_ZSDT0067            TYPE ZSDT0067,
         WA_ZSDT0068            TYPE ZSDT0068.

  DATA : VL_NR_OV       TYPE ZSDT0067-NR_OV,
         VL_NRO_SOL_OV  TYPE ZSDT0067-NRO_SOL_OV.


  CLEAR: WA_ZIB_CONT_FRETE,
         WA_ZIB_CONT_FRETE_PARC.


  LOOP AT T_ZIB_CONT_FRETE INTO WA_ZIB_CONT_FRETE.

    CLEAR: WA_ZSDT0067, VL_NR_OV, VL_NRO_SOL_OV.

    SELECT SINGLE NR_OV NRO_SOL_OV
      FROM ZSDT0067
      INTO (VL_NR_OV,VL_NRO_SOL_OV)
     WHERE CH_REFERENCIA = WA_ZIB_CONT_FRETE-CH_REFERENCIA.

    MOVE-CORRESPONDING WA_ZIB_CONT_FRETE TO WA_ZSDT0067.

    WA_ZSDT0067-NR_OV         = VL_NR_OV.
    WA_ZSDT0067-NRO_SOL_OV    = VL_NRO_SOL_OV.
    WA_ZSDT0067-RG_ATUALIZADO = '0'.

    MODIFY ZSDT0067 FROM WA_ZSDT0067.

  ENDLOOP.

  LOOP AT T_ZIB_CONT_FRETE_PARC INTO WA_ZIB_CONT_FRETE_PARC.

    CLEAR: WA_ZSDT0068.

    MOVE-CORRESPONDING WA_ZIB_CONT_FRETE_PARC TO WA_ZSDT0068.

    MODIFY ZSDT0068 FROM WA_ZSDT0068.

  ENDLOOP.



  COMMIT WORK.

ENDFUNCTION.
