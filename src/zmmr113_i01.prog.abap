MODULE USER_COMMAND_0100 INPUT.
  CASE OK-CODE.
    WHEN C_SEARCH.
      PERFORM F_BUSCA_DADOS.
    WHEN C_CANCEL.
      SET SCREEN 0.
    WHEN C_BACK.
      SET SCREEN 0.
    WHEN C_EXIT.
      LEAVE PROGRAM.
  ENDCASE.
ENDMODULE.
