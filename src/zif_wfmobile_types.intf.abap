INTERFACE ZIF_WFMOBILE_TYPES
  PUBLIC .

  TYPES:
    "//Purchase Order
    BEGIN OF TY_PURCHASE_ORDER,
      ID                 TYPE C LENGTH 12,
      DESCRIPTION        TYPE C LENGTH 255,
      DATE               TYPE C LENGTH 10,
      TIME               TYPE C LENGTH 6,
      TEXT               TYPE C LENGTH 225,
      TOTAL_VALUE        TYPE P LENGTH 13 DECIMALS 2,
      TOTAL_VALUE_STRING TYPE C LENGTH 30,
      VENDOR             TYPE C LENGTH 255,
    END OF TY_PURCHASE_ORDER,

    BEGIN OF TY_PURCHASE_ITEMS,
      ID                    TYPE C LENGTH 12,
      ITEM                  TYPE C LENGTH 5,
      DATA_PEDIDO           TYPE C LENGTH 10,
      MATERIAL              TYPE C LENGTH 255,
      BRANCH                TYPE C LENGTH 255,
      QUANTITY              TYPE P LENGTH 13 DECIMALS 0,
      QUANTITY_STRING       TYPE C LENGTH 30,
      VALUE                 TYPE P LENGTH 13 DECIMALS 2,
      VALUE_STRING          TYPE C LENGTH 30,
      UNIDADE               TYPE C LENGTH 10,
      VALOR_UNITARIO        TYPE P LENGTH 13 DECIMALS 2,
      VALOR_UNITARIO_STRING TYPE C LENGTH 30,
      ACCOUNTING_ACCOUNT    TYPE C LENGTH 255,
      CLASSIFICATION        TYPE C LENGTH 255,
      COST_CENTER           TYPE C LENGTH 255,
      ORDEM                 TYPE C LENGTH 255,
      IMOBILIZADO           TYPE C LENGTH 255,
    END OF TY_PURCHASE_ITEMS,

    T_PURCHASE_ORDERS      TYPE TABLE OF TY_PURCHASE_ORDER
      WITH KEY ID,

    T_PURCHASE_ORDER_ITEMS TYPE TABLE OF TY_PURCHASE_ITEMS
      WITH KEY ID ITEM,

    "//Material reservation
    BEGIN OF TY_RESERVATION,
      ID          TYPE C LENGTH 12,
      DESCRIPTION TYPE C LENGTH 255,
      COST_CENTER TYPE C LENGTH 255,
      DATE        TYPE C LENGTH 10,
      TOTAL_VALUE TYPE C LENGTH 30,
      REQUISITOR  TYPE C LENGTH 255,
    END OF TY_RESERVATION,

    BEGIN OF TY_RESERVATION_ITEM,
      ID          TYPE C LENGTH 12,
      ITEM        TYPE C LENGTH 5,
      MATERIAL    TYPE C LENGTH 255,
      BRANCH      TYPE C LENGTH 255,
      QUANTIDADE  TYPE C LENGTH 30,
      ACCOUNT     TYPE C LENGTH 255,
      VALUE       TYPE C LENGTH 30,
      UNIDADE     TYPE C LENGTH 10,
      COST_CENTER TYPE C LENGTH 255,
      REQUISITOR  TYPE C LENGTH 255,
    END OF TY_RESERVATION_ITEM,

    T_RESERVATIONS      TYPE TABLE OF TY_RESERVATION
      WITH KEY ID,

    T_RESERVATION_ITEMS TYPE TABLE OF TY_RESERVATION_ITEM
      WITH KEY ID ITEM,

    "//Purchase Requisition
    BEGIN OF TY_PURCHASE_REQUISITION,
      ID              TYPE C LENGTH 12,
      DATA_REQUISICAO TYPE C LENGTH 10,
      DESCRIPTION     TYPE C LENGTH 255,
      DATE            TYPE C LENGTH 10,
      TIME            TYPE C LENGTH 6,
      COST_CENTER     TYPE C LENGTH 255,
      TEXT            TYPE C LENGTH 225,
      TOTAL_VALUE     TYPE C LENGTH 30,
      VENDOR          TYPE C LENGTH 255,
    END OF TY_PURCHASE_REQUISITION,

    BEGIN OF TY_PURCHASE_REQUISITION_ITEM,
      ID                 TYPE C LENGTH 12,
      ITEM               TYPE C LENGTH 5,
      MATERIAL           TYPE C LENGTH 255,
      DATA_REQUISICAO    TYPE C LENGTH 10,
      BRANCH             TYPE C LENGTH 255,
      QUANTIDADE         TYPE C LENGTH 30,
      UNIDADE            TYPE C LENGTH 10,
      VALOR_UNITARIO     TYPE C LENGTH 30,
      VALUE              TYPE C LENGTH 30,
      ACCOUNTING_ACCOUNT TYPE C LENGTH 255,
      CLASSIFICATION     TYPE C LENGTH 255,
      COST_CENTER        TYPE C LENGTH 255,
      ORDEM              TYPE C LENGTH 255,
      IMOBILIZADO        TYPE C LENGTH 255,
      REQUISITANTE       TYPE C LENGTH 255,
    END OF TY_PURCHASE_REQUISITION_ITEM,

    T_PURCHASE_REQUISITION      TYPE TABLE OF TY_PURCHASE_REQUISITION
      WITH KEY ID,

    T_PURCHASE_REQUISITION_ITEM TYPE TABLE OF TY_PURCHASE_REQUISITION_ITEM
      WITH KEY ID ITEM,

    "//Advance to supplier
    BEGIN OF TY_ADVANCE_SUPPLIERS,
      ID          TYPE C LENGTH 12,
      DESCRIPTION TYPE C LENGTH 255,
      DATE        TYPE C LENGTH 10,
      COMPANY     TYPE C LENGTH 30,
      TOTAL_VALUE TYPE C LENGTH 30,
    END OF TY_ADVANCE_SUPPLIERS,

    BEGIN OF TY_ADVANCE_SUPPLIERS_ITEM,
      ID         TYPE C LENGTH 12,
      DOCUMENT   TYPE C LENGTH 10,
      VALUE      TYPE C LENGTH 30,
      VENDOR     TYPE C LENGTH 35,
      MATERIAL   TYPE C LENGTH 255,
      REASON     TYPE C LENGTH 255,
      NEGOTIATOR TYPE C LENGTH 80,
    END OF TY_ADVANCE_SUPPLIERS_ITEM,

    T_ADVANCE_SUPPLIERS     TYPE TABLE OF TY_ADVANCE_SUPPLIERS
      WITH KEY ID,

    T_ADVANCE_SUPPLIER_ITEM TYPE TABLE OF TY_ADVANCE_SUPPLIERS_ITEM
     WITH KEY ID DOCUMENT,

    "//Tax payments
    BEGIN OF TY_TAX_PAYMENTS,
      ID          TYPE C LENGTH 12,
      DESCRIPTION TYPE C LENGTH 255,
      DATE        TYPE C LENGTH 10,
      COMPANY     TYPE C LENGTH 30,
      DEPARTMENT  TYPE C LENGTH 255,
      TOTAL_VALUE TYPE C LENGTH 30,
    END OF TY_TAX_PAYMENTS,

    BEGIN OF TY_TAX_PAYMENT_ITEM,
      ID          TYPE C LENGTH 12,
      DOCUMENT    TYPE C LENGTH 10,
      VALUE       TYPE C LENGTH 30,
      DESCRIPTION TYPE C LENGTH 35,
      OBSERVATION TYPE C LENGTH 100,
    END OF TY_TAX_PAYMENT_ITEM,

    T_TAX_PAYMENTS      TYPE TABLE OF TY_TAX_PAYMENTS
      WITH KEY ID,

    T_TAX_PAYMENT_ITEMS TYPE TABLE OF TY_TAX_PAYMENT_ITEM
     WITH KEY ID DOCUMENT,

    "//Invoices
    BEGIN OF TY_INVOICES,
      ID          TYPE C LENGTH 12,
      DESCRIPTION TYPE C LENGTH 255,
      DATE        TYPE C LENGTH 10,
      COMPANY     TYPE C LENGTH 30,
      TOTAL_VALUE TYPE P LENGTH 13 DECIMALS 3,
    END OF TY_INVOICES,

    BEGIN OF TY_INVOICE_ITEMS,
      ID          TYPE C LENGTH 12,
      INVOICE     TYPE C LENGTH 15,
      VALUE       TYPE P LENGTH 13 DECIMALS 3,
      DESCRIPTION TYPE C LENGTH 35,
      SHIP        TYPE C LENGTH 255,
      VENDOR      TYPE C LENGTH 50,
    END OF TY_INVOICE_ITEMS,

    T_INVOICES      TYPE TABLE OF TY_INVOICES
      WITH KEY ID,

    T_INVOICE_ITEMS TYPE TABLE OF TY_INVOICE_ITEMS
     WITH KEY ID INVOICE,

    "//Accounting Entries
    BEGIN OF TY_ACCOUNTING_ENTRIES,
      ID          TYPE C LENGTH 12,
      DESCRIPTION TYPE C LENGTH 255,
      DATE        TYPE C LENGTH 10,
      COMPANY     TYPE C LENGTH 30,
      TOTAL_VALUE TYPE C LENGTH 30,
    END OF TY_ACCOUNTING_ENTRIES,

    BEGIN OF TY_ACCOUNTING_ENTRY_ITEM,
      ID          TYPE C LENGTH 12,
      DOCUMENT    TYPE C LENGTH 15,
      DESCRIPTION TYPE C LENGTH 255,
      VALUE       TYPE C LENGTH 30,
    END OF TY_ACCOUNTING_ENTRY_ITEM,

    T_ACCOUNTING_ENTRIES    TYPE TABLE OF TY_ACCOUNTING_ENTRIES
      WITH KEY ID,

    T_ACCOUNTING_ENTRY_ITEM TYPE TABLE OF TY_ACCOUNTING_ENTRY_ITEM
     WITH KEY ID DOCUMENT,


    "//PM Orders
    BEGIN OF TY_PERMITS,
      ID          TYPE C LENGTH 12,
      CENTRO      TYPE C LENGTH 15,
      DESCRIPTION TYPE C LENGTH 40,
      TEXT        TYPE C LENGTH 40,
      VALUE       TYPE C LENGTH 30,
      EQUIPMENT   TYPE C LENGTH 70,
      DATE        TYPE C LENGTH 10,
    END OF TY_PERMITS,

    T_PERMITS TYPE TABLE OF TY_PERMITS
     WITH KEY ID,

    "//Payroll
    BEGIN OF TY_PAYROLL,
      ID          TYPE C LENGTH 12,
      COMPANY     TYPE C LENGTH 30,
      DESCRIPTION TYPE C LENGTH 255,
      DATE        TYPE C LENGTH 10,
      AREA        TYPE C LENGTH 20,
      PAYMENTKIND TYPE C LENGTH 10,
      VALUE       TYPE C LENGTH 30,
    END OF TY_PAYROLL,

    T_PAYROLL TYPE TABLE OF TY_PAYROLL
                WITH KEY ID,

    "//PM Orders
    BEGIN OF TY_SUPPLEMENTATION,
      ID          TYPE C LENGTH 12,
      CENTRO      TYPE C LENGTH 15,
      DESCRIPTION TYPE C LENGTH 40,
      VALUE       TYPE C LENGTH 30,
      REQUESTER   TYPE SY-UNAME,
      DATE        TYPE C LENGTH 10,
    END OF TY_SUPPLEMENTATION,

    T_SUPPLEMENTATIONS TYPE TABLE OF TY_SUPPLEMENTATION
     WITH KEY ID,

    BEGIN OF TY_CREDIT_LIMIT,
      ID       TYPE C LENGTH 12,
      COMPANY  TYPE C LENGTH 255,
      ORDEM    TYPE C LENGTH 10,
      ROMANEIO TYPE C LENGTH 10,
      MATERIAL TYPE C LENGTH 255,
      CUSTOMER TYPE C LENGTH 255,
      VALUE    TYPE C LENGTH 30,
    END OF TY_CREDIT_LIMIT,

    T_CREDIT_LIMIT TYPE TABLE OF TY_CREDIT_LIMIT
     WITH KEY ID,

    BEGIN OF TY_RETURN,
      TYPE    TYPE BAPIRET2-TYPE,
      MESSAGE TYPE BAPIRET2-MESSAGE,
    END OF TY_RETURN.

  CONSTANTS:
    BEGIN OF MESSAGES_TYPE,
      SUCCESS VALUE 'S',
      ERROR   VALUE 'E',
    END OF MESSAGES_TYPE.

  TYPES WORKFLOW_INBOX TYPE TABLE OF SWWWLHEAD.
  TYPES BAPI_DETAILS   TYPE TABLE OF BAPIRETURN.
ENDINTERFACE.
