CLASS ZCL_NBIOBSP_EXTRACTION IMPLEMENTATION.


  method CONSTRUCTOR.

    IF I_OLE_EXTRACTION IS INITIAL.
      RAISE EXCEPTION TYPE ZCX_NBIOBSP
        EXPORTING
          TEXTID = VALUE #( MSGID  = ZCX_NBIOBSP=>ZCX_REF_OBJ_OLE_EXTRACTION-MSGID
                            MSGNO  = ZCX_NBIOBSP=>ZCX_REF_OBJ_OLE_EXTRACTION-MSGNO )
