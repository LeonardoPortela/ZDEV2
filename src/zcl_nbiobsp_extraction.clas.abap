          MSGNO  = ZCX_NBIOBSP=>ZCX_ERRO_METODO-MSGNO
          MSGTY  = 'E'
          MSGV1  = 'ZIF_NBIOBSP_EXTRACTION~CAPTURE'
          MSGV2  = SY-MSGV2
          MSGV3  = CONV #( ME->ZIF_NBIOBSP_EXTRACTION~ERRORDESCRIPTION ).
    ENDIF.

  ENDMETHOD.


