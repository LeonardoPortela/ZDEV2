  METHOD ZIF_NBIOBSP_EXTRACTION~CAPTURE.

    R_EXTRACTION = ME.
    CALL METHOD OF ME->ZIF_NBIOBSP_EXTRACTION~EXTRACTION 'Capture' EXPORTING #1 = I_NPURPOSE.

    IF ME->ZIF_NBIOBSP_EXTRACTION~GET_ERRORCODE( )->ERRORCODE IS NOT INITIAL.

      ME->ZIF_NBIOBSP_EXTRACTION~GET_ERRORDESCRIPTION( ).

      SY-MSGV2 = ME->ZIF_NBIOBSP_EXTRACTION~ERRORCODE.
      CONDENSE SY-MSGV2 NO-GAPS.

      RAISE EXCEPTION TYPE ZCX_NBIOBSP
        EXPORTING
          TEXTID = VALUE #( MSGID  = ZCX_NBIOBSP=>ZCX_ERRO_METODO-MSGID
                            MSGNO  = ZCX_NBIOBSP=>ZCX_ERRO_METODO-MSGNO
                            ATTR1  = 'ZIF_NBIOBSP_EXTRACTION~CAPTURE'
                            ATTR2  = CONV #( SY-MSGV2 )
                            ATTR3  = ME->ZIF_NBIOBSP_EXTRACTION~ERRORDESCRIPTION )
