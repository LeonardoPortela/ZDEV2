     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FUNCTIONALITY&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Dieser Baustein dient zum Anlegen eines neuen Equipments.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;HINTS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE &apos;ITOB_BAPI_HINTS&apos; OBJECT DOKU ID TX LANGUAGE D</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Spezielle Hinweise zum Baustein BAPI_EQUI_CREATE:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Eine externe Nummernvergabe kann durch Setzen des Parameters</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>EXTERNAL_NUMBER erreicht werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Über den Parameter VALID_DATE kann das Gültig-Ab-Datum für das</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>anzulegende Equipment bestimmt werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Über den Parameter DATA_INSTALL kann das anzulegende Equipment in einen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Technischen Platz oder ein übergeordnetes Equipment eingebaut werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Über den Parameter DATA_FLEET können fahrzeugspezifische Daten</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>mitgegeben werden. Diese Daten werden über den Parameter</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>DATA_FLEET_EXP an den</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Aufrufer zurückgegeben.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FURTHER_SOURCES_OF_INF&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
