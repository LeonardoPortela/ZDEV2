PROCESS BEFORE OUTPUT.
  MODULE CLEAR.
***
  MODULE CRL_REG_CHANGE_TC_ATTR.
  LOOP AT   IT_REG
       WITH CONTROL CRL_REG
       CURSOR CRL_REG-CURRENT_LINE.
    MODULE CRL_REG_GET_LINES.
  ENDLOOP.
***
  MODULE STATUS_0100.
*
PROCESS AFTER INPUT.
***
  LOOP AT IT_REG.
    CHAIN.
      FIELD IT_REG-POINT.
      FIELD IT_REG-PTTXT.
      FIELD IT_REG-ATNAM.
      FIELD IT_REG-MRNGU.
      FIELD IT_REG-MRMAC.
      FIELD IT_REG-MRMIC.
      FIELD IT_REG-RDANT.
      FIELD IT_REG-RDATU.
      MODULE CRL_REG_MODIFY ON CHAIN-REQUEST.
    ENDCHAIN.
  ENDLOOP.

  MODULE CRL_REG_USER_COMMAND.
***
  MODULE USER_COMMAND_0100.

  CHAIN.
    FIELD  MHIO-ADDAT    MODULE VERIFICA_DATA.
    FIELD  MHIO-ADTIME   MODULE VERIFICA_HORA.
    FIELD: MHIO-WARPL,
           MHIO-QMNUM    MODULE VERIFICA_NOTA.
  ENDCHAIN.

  CHAIN.
    FIELD: V_EQUNR MODULE ADD_ITENS.
  ENDCHAIN.
  MODULE LIMPA.

PROCESS ON VALUE-REQUEST.
  FIELD: MHIO-QMNUM
         MODULE VALUE_FOR_QMNUM.
