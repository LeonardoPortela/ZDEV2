*----------------------------------------------------------------------*
***INCLUDE ZMMR105_0114.
*----------------------------------------------------------------------*

*&---------------------------------------------------------------------*
*&      Module  STATUS_0114  OUTPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE STATUS_0114 OUTPUT.

  IF VG_TELA_0114 IS INITIAL.
    VG_TELA_0114 = TL_0119.
    TABRODO-ACTIVETAB = OK_TABVEIC.
  ENDIF.

ENDMODULE.

*&---------------------------------------------------------------------*
*&      Module  USER_COMMAND_0114  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE USER_COMMAND_0114 INPUT.

  CASE OK_CODE.
    WHEN OK_TABVEIC.
      VG_TELA_0114 = TL_0119.
      TABRODO-ACTIVETAB = OK_TABVEIC.
    WHEN OK_TABMOTO.
      VG_TELA_0114 = TL_0120.
      TABRODO-ACTIVETAB = OK_TABMOTO.
  ENDCASE.

ENDMODULE.
