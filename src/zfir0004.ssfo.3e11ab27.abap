DATA: WL_AWKEY TYPE BKPF-AWKEY.
CLEAR: WG_PIS_COFINS.

CONCATENATE WG_EKBE-BELNR WG_EKBE-GJAHR INTO WL_AWKEY.

READ TABLE TG_BKPF INTO WG_BKPF
  WITH KEY AWKEY = WL_AWKEY
           BUKRS = WG_EKKO-BUKRS
           GJAHR = WG_EKBE-GJAHR.

IF SY-SUBRC IS INITIAL.
  READ TABLE TG_WITH_ITEM INTO WG_WITH_ITEM
    WITH KEY WITHT = 'CT'
             belnr = wg_bkpf-belnr
             gjahr = wg_bkpf-gjahr.

ENDIF.
WG_PIS_COFINS = WG_WITH_ITEM-WT_QBSHH.

















