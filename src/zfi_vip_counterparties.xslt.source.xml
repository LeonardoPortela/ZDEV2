<?sap.transform simple?>
<tt:transform xmlns:tt="http://www.sap.com/transformation-templates" xmlns:ddic="http://www.sap.com/abapxml/types/dictionary" xmlns:def="http://www.sap.com/abapxml/types/defined">
  <tt:root name="ROOT" type="?"/>
  <tt:root name="COMPANY_NODE" type="ddic:ZFIS_VIP_COUNTERPARTIES"/>
  <tt:template>
    <company>
      <companyType tt:value-ref=".COMPANY_NODE.COMPANYTYPE"/>
      <companyCode tt:value-ref=".COMPANY_NODE.COMPANYCODE"/>
      <shortName tt:value-ref=".COMPANY_NODE.SHORTNAME"/>
      <fullName tt:value-ref=".COMPANY_NODE.FULLNAME"/>
      <currency tt:value-ref=".COMPANY_NODE.CURRENCY"/>
      <isInactive tt:value-ref=".COMPANY_NODE.ISINACTIVE"/>
      <arAccount tt:value-ref=".COMPANY_NODE.ARACCOUNT"/>
      <apAccount tt:value-ref=".COMPANY_NODE.APACCOUNT"/>
      <vatNo tt:value-ref=".COMPANY_NODE.VATNO"/>
      <address1 tt:value-ref=".COMPANY_NODE.ADDRESS1"/>
      <address2 tt:value-ref=".COMPANY_NODE.ADDRESS2"/>
      <address3 tt:value-ref=".COMPANY_NODE.ADDRESS3"/>
      <countryCode tt:value-ref=".COMPANY_NODE.COUNTRYCODE"/>
      <externalRef tt:value-ref=".COMPANY_NODE.EXTERNALREF"/>
      <referenceCode tt:value-ref=".COMPANY_NODE.REFERENCECODE"/>
      <vendorUserProperties>
        <tt:loop ref=".COMPANY_NODE.VENDORUSERPROPERTIES">
          <vendorUserProperty>
            <fieldName tt:value-ref="FIELDNAME"/>
            <fieldID tt:value-ref="FIELDID"/>
            <value tt:value-ref="VALUE"/>
          </vendorUserProperty>
        </tt:loop>
      </vendorUserProperties>
    </company>
  </tt:template>
</tt:transform>
