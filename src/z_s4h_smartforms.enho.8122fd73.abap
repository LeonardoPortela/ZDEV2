"Name: \TY:CL_COS_UTILITIES\ME:IS_S4H\SE:END\EI
ENHANCEMENT 0 Z_S4H_SMARTFORMS.

  SELECT COUNT(*)
    FROM TVARVC
    WHERE NAME EQ 'USER_SMARTFORMS'
    AND LOW EQ SY-UNAME.
  IF SY-SUBRC IS INITIAL AND ( SY-TCODE = 'SMARTFORMS' OR SY-TCODE = 'SE71' ).
    RV_IS_S4H = ABAP_FALSE.
  ENDIF.

ENDENHANCEMENT.
